{
  "name": "royalty",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:prod": "tsc && vite build --mode production",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "deploy": "npm run build:prod && wrangler publish",
    "deploy:staging": "npm run build:prod && wrangler publish --env staging",
    "clean": "rimraf dist node_modules/.vite .wrangler",
    "build:clean": "npm run clean && npm run build:prod",
    "analyze": "cross-env ANALYZE=true npm run build:prod",
    "preinstall": "npm config set legacy-peer-deps true"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.3.0",
    "@supabase/supabase-js": "^2.49.1",
    "@tailwindcss/vite": "^4.0.14",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-paystack": "^4.0.3",
    "react-router-dom": "^6.20.0",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230518.0",
    "@eslint/js": "^9.21.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.3.4",
    "cross-env": "^7.0.3",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "postcss-import": "^16.1.0",
    "rimraf": "^5.0.1",
    "rollup-plugin-visualizer": "^5.9.2",
    "terser": "^5.17.7",
    "typescript": "~5.3.3",
    "typescript-eslint": "^7.0.0",
    "vite": "^5.1.4",
    "vite-plugin-compression2": "^0.10.1",
    "wrangler": "^3.0.0"
  }
}
